{
  "manifest_version": 3,
  "name": "Chinese Extension",
  "version": "1.0.0",
  "description": "An extension for reading Chinese on the web.",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
	    "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "storage"
  ]
}